Algoritmo TresNumeros
	Escribir "Ingresar primer numero";
	Leer a;
	Escribir "Ingresar segundo numero";
	Leer b;
	Escribir "Ingresar tercer numero";
	Leer c; 
	
	promedio<- (a*b*c)/3;
	suma<- (a+b+c);
	dobleA<- 2*a;
	tripleB<- 3*b;
	cuadradoC<- c*c*c*c;
	
	Escribir "Promedio ", promedio;
	Escribir "Suma ", suma;
	Escribir "Doble del primer n° ", dobleA;
	Escribir "Triple del segundo n° ", tripleB;
	Escribir "Cuadrado del tercer n° ", cuadradoC;
	
	
	
FinAlgoritmo
