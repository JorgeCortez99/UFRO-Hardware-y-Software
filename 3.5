Algoritmo empleadoTeclado
	
	Escribir "Ingrese sueldo base";
	Leer sb;
	Escribir "Ingrese cantidad de horas extra";
	Leer he;
	Escribir "Ingrese monto por horas extra";
	Leer mhe;
	
	hExtra<- (he*mhe);
	sTotal<- (hExtra + sb);
	dsc<- (sTotal*20)/100;
	sLiquido<- sTotal - dsc;
	
	Escribir "Sueldo básico ", sb;
	Escribir "Total descuento ", dsc;
	Escribir "Sueldo total ganado ", sLiquido;
	
	
	
	
	
FinAlgoritmo
